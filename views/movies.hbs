<h2>IronHack</h2>
<div class="row">
{{#each movies}}
  <div class="col-md-4">
    <div class="card">
      <img class="card-img-top" src="{{this.image}}" alt="{{this.title}}" />
      <div class="card-body">
        <h5 class="card-title">
          {{this.title}}
        </h5>
        <a href="/movie/{{this.id}}" >See more</a>
      </div>
    </div>
  </div>
{{/each}}
</div>